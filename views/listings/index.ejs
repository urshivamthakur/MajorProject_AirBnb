<% layout("/layouts/boilerplate") %>

    <body>

        <h2>All Listings</h2>
        <form method="GET" action="/listings/new">
            <button>Create New Listings</button>
        </form>
        <ul>
            <% for(let listing of allListings) { %>
                <li><a href="/listings/<%=listing._id%>">
                        <%= listing.title %>
                    </a></li>
                <% } %>
        </ul>
    </body>